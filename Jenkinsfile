pipeline{
        agent any
        stages{
              stage ("Build"){
                              step{
                                  echo "I am building a project"
                              }
              }
              stage ("Test"){
                            step{
                                echo "I am testing the project"
                            }
              }
              stage ("deploy"){
                              step{
                                  echo "I am in deployment"
                              }
              }            
        }
}
